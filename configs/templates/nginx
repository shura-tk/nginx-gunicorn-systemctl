server {
    listen 80;
    server_name {{.NameProject}}.vbti.local;
    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /opt/ngs/development/{{.NameProject}}/{{.NameProject}}/{{.NameProject}};
    }
    location / {
        include proxy_params;
        proxy_pass http://unix:/run/ngs_{{.NameProject}}.sock;
    }
}