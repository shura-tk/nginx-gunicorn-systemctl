server {
    listen 80;
    server_name {{.NameProject}}.{{.Domain}};
    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root {{.PathPjt}}{{.NameProject}}/{{.NameProject}}/;
    }
    location / {
        include proxy_params;
        proxy_pass http://unix:/run/{{.Prefix}}{{.NameProject}}.sock;
    }
}