[Unit]
Description={{.Prefix}}{{.NameProject}} daemon
Requires={{.Prefix}}{{.NameProject}}.socket
After=network.target

[Service]
WorkingDirectory={{.PathPjt}}{{.NameProject}}/{{.NameProject}}
ExecStart={{.PathPjt}}{{.NameProject}}/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/{{.Prefix}}{{.NameProject}}.sock \
          {{.NameProject}}.wsgi:application

[Install]
WantedBy=multi-user.target